#!/usr/bin/env python

import base64
import io
import sys
import tempfile
import zipfile

main = b''

data = io.BytesIO(base64.b64decode(main))

zf = zipfile.ZipFile(data)

with tempfile.TemporaryDirectory() as temp_path:
    zf.extractall(temp_path)

    sys.path.insert(0, temp_path)

    from eruza.app import main
    main()
